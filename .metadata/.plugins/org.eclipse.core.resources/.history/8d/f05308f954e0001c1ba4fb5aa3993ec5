package sequencial;

import java.util.Scanner;

public class MediaPonderada {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		//int, float, double, char, bool, String -> primitivos
		float n1, n2, n3, n4;
		System.out.println("Digite N1: ");
		n1 = sc.nextFloat();
		n2 = sc.nextFloat();
		n3 = sc.nextFloat();
		n4 = sc.nextFloat();
		
		sc.close();

	}

}
