package sequencial;

import java.util.Scanner;

public class DescontoINSS {

	public static void main(String[] args) {
		
		double salarioBruto, descontoINSS, salarioLiquido;
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Digite o salário bruto: ");
		salarioBruto = sc.nextDouble();
		
		descontoINSS = salarioBruto * (1-0.11);
				
		System.out.printf("O salário bruto é de R$%.2f e o salário líquido é de R$%.2f, "
				+"com o desconto de %.0f por cento do INSS.",salarioBruto, salarioLiquido);
		
		sc.close();

	}

}
